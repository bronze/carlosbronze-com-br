---
const { quote, author } = Astro.locals // Ensure Astro.locals is used
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Random Quote</title>
  </head>
  <body>
    <h1>{quote}</h1>
    <p>&mdash; {author}</p>
  </body>
</html>
