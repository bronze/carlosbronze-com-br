{
  "useTabs": false,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "bracketSameLine": true,
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "**/*.astro",
      "options": {"parser": "astro"}
    }
  ]
}
